### VCD ###

## General ##
vcd_max_retry_timeout    = 10
vcd_config_dir           = "./.itsf/config"
vcd_cred_file            = "credentials.json"
vcd_domain               = "lvivcloud.vodafone.ua"
vcd_allow_unverified_ssl = true

## Catalog ##
vcd_catalog_name = "ITSF"

## Network ##
vcd_edge_gateway_default = "ITSF_Lviv T1"
vcd_edge_cluster_default = "vcp01-edge-cluster"

vcd_network_resources_file = "network.resources.yaml"